#!/bin/bash

mpd
xinput set-prop 8 279 1.3
feh --no-fehbg --bg-tile ~/.config/i3/bg
[ -z "$(pgrep redshift)" ] && (redshift &)

i3-msg "workspace 1; focus parent, focus parent, focus parent, focus parent, focus parent, focus parent, focus parent, focus parent, focus parent, focus parent, kill"
i3-msg "workspace 1; append_layout ~/.config/i3/workspace1.json"
chromium &
urxvt -name main &
